<Window x:Class="Group_3.CartPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Cart Page" Height="450" Width="800">
    <Grid Background="#ADD9F4">
        <ListBox x:Name="CartListBox" Margin="10" />
        <Button Content="Checkout" HorizontalAlignment="Right" Margin="10" VerticalAlignment="Bottom" Width="100" Click="CheckoutButton_Click"/>
        <Button Content="back" HorizontalAlignment="Right" Margin="0,0,690,10" VerticalAlignment="Bottom" Width="100" Click="back_Click"/>

    </Grid>
</Window>